<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css">
    <title> Find a doctor </title>
    <link href="/css/doctorCard.css" rel="stylesheet">
    <style>
      body{
        background-color: #458ff6;
      };
      .doctorBox{
        background-color: white;
        margin-bottom: 10px;
      };
      #Doctor{
        color: white;
      }

    </style>
  </head>
 
    <body> 

     <!-- ======= Doctor section ======= -->
     <section id="team" class="team section-bg">
        <div class="container">
            <a href="/" style="color: white;"><h3>Home</h3></a>
          <div class="section-title">
            <h2 id="Doctor" style="color: white;">Our doctors</h2>
            <div class="underline"></div>
            <p style="color: white;">Here are the list of world class certified Doctors!</p>
          </div>
          <div class="row">
  
            <div class="col-lg-6">
                <!-- Demo doctor-->

             <% docArray.forEach(function(item) { %>

              <div class="member d-flex align-items-start"  class="doctorBox">
                <div class="teampic"><img src=<%=`https://xsgames.co/randomusers/avatar.php?g=male`%> class="img-fluid" alt=""></div>
                <div class="member-info">
                  <h4>Name: <%= item.name %></h4>
                  <h3><%= item.speciality %></h3>
                  <p><%= item.experience %></p>
                  <p>Phone Number: <%= item.phone %> </p>
                  <p>Social Medias:<%= item.address %></p>
                  
                  <div class="social">
                    <a href=""><i class="bi bi-facebook"></i></a>
                    <a href=""><i class="bi bi-instagram"></i></a>
                    <a href=""><i class="bi bi-linkedin"></i></a>
                    <a href=""> <i class="bi bi-whatsapp"></i> </a>
                  </div>
                </div>
              </div>

             <% }) %>
              
            </div>
            <!-- Insert doctors -->
            

  
      </section><!-- End Team Section -->

   
    <!-- Option 1: Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

    
  </body>
</html>